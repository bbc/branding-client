{
    "name": "bbc/branding-client",
    "description": "PHP Branding client",
    "license": "Apache-2.0",
    "type": "library",
    "autoload": {
        "psr-4": { "BBC\\BrandingClient\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\BBC\\BrandingClient\\": "tests/" }
    },
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.0",
        "mustache/mustache": "~2.5",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "squizlabs/php_codesniffer": "^2.9",
        "symfony/cache": "^3.2"
    },
    "config": {
        "sort-packages": true
    }
}
